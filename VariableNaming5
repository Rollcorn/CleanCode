1)  value - nodeValue
2)  next - nextNode
3)  prev - prevNode

   class Node
    {
        public int value;
        public Node next;
        public Node prev;

        public Node(int _value) 
        { 
        value = _value; 
        next = null;
        prev = null;
        }
    }

4) tmp = linkOnNextNode
    Node tmp = node.next;

